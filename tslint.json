{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "interface-name": [
            false
        ],
        "prefer-const": true,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "no-bitwise": false,
        "forin": false,
        "max-line-length": [
            false
        ],
        "max-classes-per-file": false,
        "no-console": [
            false
        ],
        "no-shadowed-variable": false
    }
}